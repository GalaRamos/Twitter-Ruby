<h1>Tweets Listing</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Content</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% @tweets.each do |tweet| %>
      <tr>
        <td><%= tweet.id %></td>
        <td><%= tweet.content %></td>
        <td><%= time_ago_in_words tweet.created_at %></td>
        <td><%= link_to "Delete", "/tweets/#{tweet.id}", method: :delete %> </td>
        <td><%= link_to "Show", "/tweets/#{tweet.id}" %></td>
        <td><%= link_to "Edit", "/tweets/#{tweet.id}/edit" %> </td>
      </tr>
    <%end%>
  </tbody>
</table>
